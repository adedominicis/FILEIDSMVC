@model FILEIDSMVC.Models.RegistroArchivoModel

@{
    ViewBag.Title = "RegistrarArchivo";

}

<h2>@ViewBag.Title.</h2>

@using (Html.BeginForm("DespuesRegistro", "Home", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>Registro de nuevos archivos</h4>
    @Html.DropDownListFor(m => m.IdExtension,(List<SelectListItem>)Model.Extensiones)
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Registrar" />
        </div>
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}



