        <div class="container body-content">
            <br />
            <br />
            <br />
            <br />
            <br />
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - FILEIDS-WEB / ADDR / AISV</p>
            </footer>
        </div>
    </div>


