<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title FILEIDS-WEB</title>
    @Styles.Render("~/Content/css")
    @*@Scripts.Render("~/bundles/modernizr")*@
    @*Font Awesome*@
    <script src="https://kit.fontawesome.com/ad96a6c395.js" crossorigin="anonymous"></script>
</head>
<body>

    <div class="page-holder bg-cover bg-custom2">

        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">

            <a class="navbar-brand" href="/Home/Index">FILEIDS</a>

            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link" @Html.ActionLink("Proyectos", "Proyectos", "Proyectos")</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" @Html.ActionLink("Design Review", "Index", "DesignReview")</a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" @Html.ActionLink("Busqueda", "RegistrarArchivo", "Home")</a>
                    </li>


                </ul>

            </div>
        </nav>

        <div class="container body-content">
            <br />
            <br />
            <br />
            <br />
            <br />
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - FILEIDS-WEB / ADDR / AISV</p>
            </footer>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @*Sweetalert2*@
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @RenderSection("scripts", required: false)

</body>

